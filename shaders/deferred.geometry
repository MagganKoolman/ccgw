#version 420 core

layout(triangles) in;
layout(triangle_strip, max_vertices = 3) out;

in vec3 posOut[];
in vec3 normalOut[];
in vec2 colorOut[];

out vec2 colorOut2;
out vec3 normalOut2;

void main()
{
	for (int i = 0; i < 3; i++)
	{
		gl_Position = vec4(posOut[i], 1.0);
		colorOut2 = colorOut[i];
		normalOut2 = normalOut[i];
		EmitVertex();
	}
	EndPrimitive();
	
}